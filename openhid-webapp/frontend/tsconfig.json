{
  "compilerOptions": {
    "target": "es2015",
    "lib": [
      "dom",
      "es5",
      "es2015",
      "es2015.promise",
      "es2015.collection",
      "es2015.generator",
      "es2015.core",
      "es2016",
      "es2016.array.include",
      "es2017",
      "es2017.object"
    ],
    "module": "commonjs",
    "typeRoots": [
      "node_modules/@types"
    ],
    "declaration": false,
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noEmitHelpers": false,
    "sourceMap": false,
    "strictNullChecks": false,
    "jsx": "react"
  },
  "compileOnSave": false,
  "buildOnSave": false
}